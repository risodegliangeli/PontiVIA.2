# Deletes all cached build data for all projects
rm -rf ~/Library/Developer/Xcode/DerivedData/


# Navigate to the iOS directory
cd ios

# Remove installed Pods and lock file
rm -rf Pods
rm Podfile.lock

# De-integrate (un-hook) CocoaPods from the Xcode project
pod deintegrate

# Clean the CocoaPods cache (optional, but good for a deep clean)
pod cache clean --all

# Reinstall dependencies
pod install

# Go back to your project root
cd ..






