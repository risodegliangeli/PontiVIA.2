{
  "expo": {
    "name": "PontiVIA!",
    "description": "Il calendario smart che trova i ponti fra le festivit√†",
    "slug": "pontivia",
    "owner": "risodegliangeli",
    "version": "0.1.3b",
    "icon": "./assets/images/pontivia-splashpage.png",
    "scheme": "cercaponti",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/pontivia-splashpage.png",
      "resizeMode": "contain",
      "backgroundColor": "#E35527"
    },
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.angelieassociati.pontivia",
      "buildNumber": "0.1.3b",
      "userInterfaceStyle": "automatic",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/pontivia-splashpage.png",
        "backgroundColor": "#E35527"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.angelieassociati.pontivia",
      "permissions": [
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      ["expo-calendar",
        {
          "calendarPermission": "L'app ha bisogno di poter accedere al calendario per gestire gli eventi",
          "remindersPermission": "L'app ha bisogno di poter accedere ai promemoria per gestire gli eventi"
        }
      ],
      ["expo-splash-screen",
        {
          "image": "./assets/images/pontivia-splashpage.png",
          "imageWidth": "204",
          "resizeMode": "contain",
          "backgroundColor": "#E35527"
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cb0a6d99-e865-4053-9126-946083815c32"
      }
    }
  }
}
